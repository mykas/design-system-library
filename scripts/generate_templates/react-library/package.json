{
  "name": "@wix/__library-name__",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.dev.wixpress.com/",
    "versionBumpStrategy": "manual",
    "preserveSemverRange": true
  },
  "scripts": {
    "build": "yoshi-library build",
    "start": "start-storybook -p 6006",
    "test": "yoshi-library test --passWithNoTests",
    "lint": "yoshi-library lint",
    "versionbump": "npm --no-git-tag-version version",
    "postversionbump": "yarn",
    "publish-storybook": "build-storybook && npx janet deploy",
    "publish-storybook-ci": "build-storybook && npx janet deploy --ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "yarn lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/api": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/cli": "^6.5.15",
    "@storybook/components": "^6.5.15",
    "@storybook/core-events": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/theming": "^6.5.15",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@wix/jest-yoshi-preset": "^6.50.3",
    "@wix/yoshi-flow-library": "^6.50.3",
    "@wix/yoshi-style-dependencies": "^6.50.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.4",
    "wix-storybook-utils": "^4.0.87"
  },
  "jest": {
    "preset": "@wix/jest-yoshi-preset"
  },
  "eslintConfig": {
    "extends": "@wix/eslint-config-yoshi"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "__library-name__"
    },
    "validations": {
      "source": [
        "lint",
        "publish-storybook-ci"
      ]
    }
  },
  "wallaby": {
    "autoDetect": true
  }
}
